{
    "name": "florianlechat/source-web-console",
    "type": "project",
    "homepage": "https://github.com/FlorianLeChat/Source-Web-Console",
    "license": "MIT",
    "authors": [
        {
            "name": "Florian Trayon",
            "homepage": "https://github.com/FlorianLeChat",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/FlorianLeChat/Source-Web-Console/issues"
    },
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",

        "twig/twig": "3.21.*",
        "twig/extra-bundle": "3.21.*",

        "react/datagram": "1.10.*",
        "nelmio/cors-bundle": "2.5.*",
        "phpseclib/phpseclib": "3.0.*",
        "league/oauth2-google": "4.0.*",
        "league/oauth2-github": "3.1.*",
        "phpstan/phpdoc-parser": "2.3.*",
        "easycorp/easyadmin-bundle": "4.23.*",
        "xpaw/php-source-query-class": "dev-master",
        "knpuniversity/oauth2-client-bundle": "2.18.*",

        "ext-ctype": "*",
        "ext-iconv": "*",

        "doctrine/annotations": "2.0.*",
        "doctrine/doctrine-bundle": "2.15.*",
        "doctrine/doctrine-migrations-bundle": "3.4.*",

        "symfony/flex": "2.8.*",
        "symfony/lock": "7.3.*",
        "symfony/form": "7.3.*",
        "symfony/intl": "7.3.*",
        "symfony/mime": "7.3.*",
        "symfony/yaml": "7.3.*",
        "symfony/cache": "7.3.*",
        "symfony/asset": "7.3.*",
        "symfony/finder": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/mailer": "7.3.*",
        "symfony/string": "7.3.*",
        "symfony/process": "7.3.*",
        "symfony/console": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/web-link": "7.3.*",
        "symfony/validator": "7.3.*",
        "symfony/serializer": "7.3.*",
        "symfony/filesystem": "7.3.*",
        "symfony/translation": "7.3.*",
        "symfony/twig-bundle": "7.3.*",
        "symfony/dom-crawler": "7.3.*",
        "symfony/http-client": "7.3.*",
        "symfony/rate-limiter": "7.3.*",
        "symfony/property-info": "7.3.*",
        "symfony/monolog-bundle": "3.10.*",
        "symfony/property-access": "7.3.*",
        "symfony/security-bundle": "7.3.*",
        "symfony/framework-bundle": "7.3.*",
        "symfony/ux-twig-component": "2.30.*",
        "symfony/expression-language": "7.3.*",
        "symfony/webpack-encore-bundle": "*"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "require": "7.3.*",
            "allow-contrib": false
        }
    },
    "require-dev": {
        "phpmd/phpmd": "2.15.*",
        "phpunit/phpunit": "11.5.*",
        "phpstan/phpstan": "1.12.*",
        "phpstan/phpstan-symfony": "1.4.*",
        "phpstan/phpstan-doctrine": "1.5.*",
        "phpstan/extension-installer": "1.4.*",

        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/debug-bundle": "7.3.*",
        "symfony/phpunit-bridge": "7.3.*",
        "symfony/web-profiler-bundle": "7.3.*",

        "squizlabs/php_codesniffer": "3.13.*",
        "doctrine/doctrine-fixtures-bundle": "4.1.*"
    }
}
